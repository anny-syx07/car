{
    "headers": [
        {
            "source": "/(.*).(webp|png|jpg|json|glb|gltf|wav|mp3)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}
